    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch) - External Terminal",
            "request": "launch",
            "console": "externalTerminal",
            "mainClass": "VC.vc"
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-vc",
            "request": "launch",
            "mainClass": "VC.vc",
            "console": "internalConsole",
            "args": "${workspace}/gcd.vc"
        }
    ]
}