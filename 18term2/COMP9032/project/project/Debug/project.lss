                                 	rcall lcd_data
                                 	rcall lcd_wait
                                     pop r16
                                 .endmacro
                                 
                                 ;@0 indicate which cup is up
                                 .macro light_cup
                                 	sbi PORTC, (7-@0)
                                 .endmacro
                                 
                                 .macro dark_cup
                                 	cbi PORTC, (7-@0)
                                 .endmacro
                                 ; all led on
                                 .macro  all_light
                                     light_cup 0
                                     light_cup 1
                                     light_cup 2
                                 .endmacro
                                 ; all led off
                                 .macro  all_dark
                                     dark_cup 0
                                     dark_cup 1
                                     dark_cup 2
                                 .endmacro
                                 ; result indicator on
                                 .macro winner_light
                                 	sbi PORTC, 7 - 7
                                 	sbi PORTC, 7 - 6
                                 	sbi PORTC, 7 - 5
                                 	sbi PORTC, 7 - 4
                                 .endmacro
                                 ; result indicator off
                                 .macro winner_dark
                                 	cbi PORTC, 7 - 7
                                 	cbi PORTC, 7 - 6
                                 	cbi PORTC, 7 - 5
                                 	cbi PORTC, 7 - 4
                                 .endmacro
                                 ;display score's digits on lcd
                                 .macro display_digits	;display @1's value on lcd, @0 is helper register
                                 	ldi @0, '0'	;@0 is to get @1's digits' ascii code
                                 	cpi @1, 100	;if @1 smaller than 100, means @1's value is 2 digits
                                 	brlo two_digits
                                 	;TODO:
                                 	loop_1:	;otherwis, get its hundreds digit
                                 		cpi @1, 100
                                 		brlo two_digits
                                 		sbci @1, 100
                                 		inc @0
                                 		rjmp loop_1
                                 	two_digits:
                                 		mov r16, @0	;load digit ascii code to r23
                                 		call lcd_data	;first digit display
                                 		call lcd_wait	;wait data transfer
                                 		ldi @0, '0'	;@0's initial value
                                 		cpi @1, 10	;@1 could be 1 digits decimal value if it smaller than 10
                                 		brlo one_digit
                                 		loop_2:
                                 			cpi @1, 10
                                 			brlo one_digit
                                 			sbci @1, 10
                                 			inc @0
                                 			rjmp loop_2
                                 			;same function as above 3 digits situation
                                 	one_digit:
                                 		mov r16, @0	;second digit display
                                 		call lcd_data
                                 		call lcd_wait	;last digit display
                                 		ldi temp1, '0'
                                 		add @1, temp1	;the last digit of @1
                                 		mov r16, @1
                                 		call lcd_data
                                 		call lcd_wait
                                 		clr @1	
                                 .endmacro
                                 
                                 
                                 
